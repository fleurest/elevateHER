{
  "name": "elevateher",
  "version": "1.0.0",
  "description": "elevateher",
  "main": "server/index.js",
  "scripts": {
    "start:client": "webpack serve --config webpack.config.js --open",
    "start:server": "node server/index.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "webpack --mode production",
    "test": "jest",
    "cypress:open": "start-server-and-test start http://localhost:3000 \"cypress open\"",
    "cypress:run": "start-server-and-test start http://localhost:3000 \"cypress run\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cytoscape": "^3.31.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "neo4j-driver": "^5.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.3.0",
    "vis-network": "^9.1.9"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "cypress": "^14.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "start-server-and-test": "^2.0.11",
    "style-loader": "^3.3.4",
    "supertest": "^7.0.0",
    "util": "^0.12.5",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "author": "Fleur Edwards",
  "license": "ISC"
}
