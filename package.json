{
  "name": "elevateher",
  "version": "1.0.0",
  "description": "elevateher",
  "main": "server/index.js",
  "scripts": {
    "start:client": "webpack serve --config webpack.config.js --open",
    "start:server": "fuser -k 3001/tcp || true && nodemon server/index.js",
    "start": "concurrently -k -n SERVER,CLIENT -c green,blue \"npm run start:server\" \"npm run start:client\"",
    "dev:clean": "sh scripts/dev-clean.sh",
    "build": "webpack --mode production",
    "test": "jest",
    "test:ci": "jest --runInBand --coverage"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "cytoscape": "^3.31.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.11.0",
    "p-limit": "^6.2.0",
    "puppeteer": "^24.7.1",
    "puppeteer-cluster": "^0.24.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.3.0",
    "simple-update-notifier": "^2.0.0",
    "vis-network": "^9.1.9"
